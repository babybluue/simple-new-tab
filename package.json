{
  "name": "Tablite",
  "description": "A clean, customizable new tab extension.",
  "private": true,
  "version": "1.0.6",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "gen:icons": "node scripts/generate-icons.mjs",
    "compile": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.cjs,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.cjs,.ts,.vue --fix",
    "format": "prettier \"**/*.{js,cjs,ts,vue,css,scss,md,json}\" --write",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/chrome": "^0.1.32",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@wxt-dev/module-vue": "^1.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.8",
    "wxt": "^0.20.6"
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa"
}
